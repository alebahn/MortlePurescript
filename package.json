{
  "name": "mortlepurescript",
  "version": "1.0.0",
  "description": "purescript port of Mortle",
  "browser": "dist/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "postinstall": "spago install",
    "build": "spago build",
    "clean": "rm -rf node_modules output .spago dist/* *.lock",
    "bundle": "spago bundle-app --main Main --to dist/app.js && parcel build assets/index.html",
    "postbundle": "cp src/index.html dist/index.html"
  },
  "author": "Aaron Lebahn",
  "license": "MIT"
}
